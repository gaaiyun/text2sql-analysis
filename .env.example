# Vanna AI 配置
VANNA_API_KEY=your_vanna_api_key_here
VANNA_MODEL=qwen-plus

# 数据库配置 (场景 1-3)
DB_HOST=your_database_host
DB_PORT=3306
DB_USER=your_database_user
DB_PASSWORD=your_database_password
DB_NAME=your_database_name

# 数据库配置 (场景 4-5)
DB_HOST_2=your_database_host_2
DB_PORT_2=3306
DB_USER_2=your_database_user_2
DB_PASSWORD_2=your_database_password_2
DB_NAME_2=your_database_name_2

# n8n 配置
N8N_WEBHOOK_URL=http://localhost:5678/webhook
N8N_API_KEY=your_n8n_api_key

# 百炼 API 配置 (阿里云)
DASHSCOPE_API_KEY=your_dashscope_api_key_here

# 可选：OpenAI API 配置
OPENAI_API_KEY=your_openai_api_key_here

# 可选：邮件配置 (用于发送报告)
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=your_email@example.com
SMTP_PASSWORD=your_email_password

# 可选：存储配置
REPORT_OUTPUT_DIR=./output
CHART_OUTPUT_DIR=./charts
