{
  "_comment": "配置模板 - 请复制此文件为 config.json 并填写真实值",
  "_warning": "切勿将包含真实凭证的 config.json 上传到 GitHub!",
  
  "database": {
    "scenario_1_3": {
      "host": "YOUR_DB_HOST",
      "port": 3306,
      "user": "YOUR_DB_USER",
      "password": "YOUR_DB_PASSWORD",
      "database": "YOUR_DB_NAME"
    },
    "scenario_4_5": {
      "host": "YOUR_DB_HOST",
      "port": 3306,
      "user": "YOUR_DB_USER",
      "password": "YOUR_DB_PASSWORD",
      "database": "YOUR_DB_NAME"
    }
  },
  
  "bailian": {
    "api_key": "YOUR_DASHSCOPE_API_KEY",
    "base_url": "https://coding.dashscope.aliyuncs.com/v1",
    "model": "qwen3.5-plus",
    "max_tokens": 4096,
    "temperature": 0.7
  },
  
  "vanna": {
    "api_key": "YOUR_VANNA_API_KEY",
    "org": "your-org-name",
    "model": "your-model"
  },
  
  "n8n": {
    "webhook_url": "http://localhost:5646/webhook/text2sql",
    "api_url": "http://localhost:5646/api/v1"
  },
  
  "output": {
    "default_format": "markdown",
    "excel_output_dir": "./output/excel",
    "word_output_dir": "./output/word",
    "html_output_dir": "./output/html"
  },
  
  "logging": {
    "level": "INFO",
    "file": "./logs/text2sql.log",
    "max_size_mb": 10,
    "backup_count": 5
  }
}
